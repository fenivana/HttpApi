{
  "name": "@fenivana/http-api",
  "version": "0.1.4",
  "description": "A cross-platform fetch API wrapper.",
  "main": "dist/HttpApi.node.js",
  "module": "dist/HttpApi.esm.js",
  "scripts": {
    "test-server": "node test/test-server",
    "test": "nyc --reporter=lcov mocha test/specs/*.js",
    "codecov": "codecov",
    "build": "rollup -c rollup.config.esm.js && rollup -c rollup.config.umd.js && rollup -c rollup.config.node.js"
  },
  "repository": "git@github.com:fenivana/HttpApi.git",
  "author": "Jiang Fengming <fenivana@noindoin.com>",
  "license": "MIT",
  "dependencies": {
    "form-data": "^2.3.2",
    "node-fetch": "^2.1.2"
  },
  "devDependencies": {
    "@koa/cors": "^2.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "codecov": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-enough": "^0.3.3",
    "koa": "^2.5.1",
    "koa-router": "^7.4.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.4"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": false
        }
      ],
      "stage-3"
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "node": true,
      "browser": true,
      "mocha": true
    }
  }
}

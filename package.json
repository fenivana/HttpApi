{
  "name": "teleman",
  "version": "0.5.0",
  "description": "A browser and node.js fetch API wrapper.",
  "main": "dist/Teleman",
  "scripts": {
    "test-server": "node test/test-server",
    "test-it": "nyc --reporter=lcov mocha test/Teleman.spec.js",
    "test": "start-server-and-test test-server 3000 test-it",
    "codecov": "codecov",
    "build": "rollup -c rollup.config.esm.mjs && rollup -c rollup.config.cjs.mjs && rollup -c rollup.config.umd.bundle.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kasha-io/teleman.git"
  },
  "bugs": {
    "url": "https://github.com/kasha-io/teleman/issues"
  },
  "homepage": "https://github.com/kasha-io/teleman#readme",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "fetch",
    "ajax",
    "RESTful"
  ],
  "author": "Jiang Fengming <jiangfengming87@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "koa-compose": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@koa/cors": "^2.2.3",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "codecov": "^3.2.0",
    "eslint": "^5.15.1",
    "eslint-config-enough": "^0.3.4",
    "form-data": "^2.3.3",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "mocha": "^6.0.2",
    "node-fetch": "^2.3.0",
    "nyc": "^13.3.0",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "start-server-and-test": "^1.7.11"
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "node": true,
      "browser": true,
      "mocha": true
    }
  }
}

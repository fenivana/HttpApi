{
  "name": "teleman",
  "version": "0.5.5",
  "description": "A browser and node.js fetch API wrapper.",
  "main": "dist/Teleman",
  "scripts": {
    "test-server": "node test/test-server",
    "test-it": "nyc --reporter=lcov mocha test/Teleman.spec.js",
    "test": "start-server-and-test test-server 3000 test-it",
    "codecov": "codecov",
    "build": "rollup -c rollup.config.esm.js && rollup -c rollup.config.cjs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kasha-io/teleman.git"
  },
  "bugs": {
    "url": "https://github.com/kasha-io/teleman/issues"
  },
  "homepage": "https://github.com/kasha-io/teleman#readme",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "fetch",
    "ajax",
    "RESTful"
  ],
  "author": "Jiang Fengming <jiangfengming87@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "koa-compose": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@koa/cors": "^3.1.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "codecov": "^3.7.2",
    "eslint": "^7.9.0",
    "eslint-config-enough": "^0.5.0",
    "form-data": "^3.0.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^9.4.0",
    "mocha": "^8.1.3",
    "node-fetch": "^2.6.1",
    "nyc": "^15.1.0",
    "rollup": "^2.27.0",
    "rollup-plugin-babel": "^4.3.3",
    "start-server-and-test": "^1.11.3"
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "node": true,
      "browser": true,
      "mocha": true
    }
  }
}
